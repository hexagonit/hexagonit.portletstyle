<?xml version="1.0" ?>
<object name="portal_controlpanel"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        i18n:domain="hexagonit.portletstyle"
        purge="False">

  <configlet title="Portlet Styles"
             action_id="portletstyles"
